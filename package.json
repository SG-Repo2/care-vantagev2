  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "yarn expo start --dev-client",
    "start:clear": "yarn expo start --clear",
    "android": "yarn expo run:android",
    "ios": "yarn expo run:ios",
    "web": "yarn expo start --web",
    "clean": "rm -rf node_modules && rm -rf android/build android/app/build && yarn cache clean && yarn install",
    "clean:metro": "rm -rf $TMPDIR/metro-* && yarn start:clear",
    "clean:all": "yarn clean && yarn clean:metro",
    "android:dev": "yarn eas build --profile development --platform android --local",
    "ios:dev": "yarn eas build --profile development --platform ios --local",
    "android:run": "yarn android:dev && yarn android",
    "ios:run": "yarn ios:dev && yarn ios",
    "metro:reset": "yarn expo start --clear",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
